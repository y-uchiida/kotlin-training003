# データベース接続の設定
spring.datasource.url=jdbc:mysql://localhost:3306/kotlin_training
spring.datasource.username=kotlin_training003_user
spring.datasource.password=kotlin_training003_pass

# JPAの設定
spring.jpa.hibernate.ddl-auto=update
spring.jpa.show-sql=true
spring.jpa.properties.hibernate.dialect=org.hibernate.dialect.MySQL8Dialect

# MySQLのドライバクラスの設定
spring.datasource.driver-class-name=com.mysql.cj.jdbc.Driver
